@model IList<WestcoastEducation.Web.Models.Classroom>

@{
    ViewBag.PageTitle = "Klassrumskurser";
}

<article class="page">
    <h1 class="page-title">Klassrumskurser</h1>
    <section class="course-container">
        @foreach (var classroom in Model)
        {
            <div>
                <a asp-action="details" asp-route-courseId=@classroom.Id>@classroom.Name - @classroom.Content</a>
            </div>
        }
    </section>
</article>